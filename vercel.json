{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "50mb" }
    }
  ],
  "routes": [
    {
      "src": "/predict",
      "dest": "api/app.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "runtime": "python3.10"
    }
  }
}
